<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sa.mybatis.cardGroup">
	<resultMap id="CardGroupModel" type="com.game.sa.data.mybatis.bean.CardGroupModel">
		<result property="userID" column="userID" />
		<result property="a" column="a" />
		<result property="b" column="b" />
		<result property="c" column="c" />
		<result property="d" column="d" />
		<result property="e" column="e" />
	</resultMap>
	<parameterMap id="CardGroupModel"
		type="com.game.sa.data.mybatis.bean.CardGroupModel">
	</parameterMap>
	<!-- 获取卡牌列表 -->
	<select id="findCardGroupModelList" resultMap="CardGroupModel">
		select * from sa_dt_card_group
	</select>
	<!-- 插入卡牌 -->
	<insert id="insertCardGroupModel" parameterMap="CardGroupModel">
		insert into
		sa_dt_card_group(userID) value(#{userID})
	</insert>
	<!-- 更新卡牌 -->
	<update id="updateCardGroupModel" parameterMap="CardGroupModel">
		update sa_dt_card_group set 
		a=#{a}, b=#{b}, c=#{c}, d=#{d}, e=#{e} 
		where userID=#{userID}
	</update>
	<!-- 删除卡牌 -->
	<delete id="deleteCardGroupModel" parameterType="int">
		delete from sa_dt_card_group where userID=#{userID}
	</delete>
</mapper>